  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "db:migrate": "sequelize-cli db:migrate --config config/config.js --migrations-path migrations",
    "db:seed": "sequelize-cli db:seed:all --config sequelize/config.js --seeders-path sequelize/seeders",
    "db:reset": "sequelize-cli db:migrate:undo:all --config sequelize/config.js && npm run db:migrate && npm run db:seed",
    "test": "jest --runInBand --detectOpenHandles"
  },
